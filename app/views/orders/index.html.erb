<p id="notice"><%= notice %></p>

<h1>Orders</h1>

<table>
  <thead>
    <tr>
      <th>Viewer</th>
      <th>Item</th>
      <th>Value</th>
      <th>Timestamp</th>
      <th>Entery</th>
      <th>Order</th>
      <th>Status</th>
      <th>Patient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.viewer %></td>
        <td><%= order.item %></td>
        <td><%= order.value %></td>
        <td><%= order.timestamp %></td>
        <td><%= order.entery %></td>
        <td><%= order.order %></td>
        <td><%= order.status %></td>
        <td><%= order.patient %></td>
        <td><%= link_to 'Show', order %></td>
        <td><%= link_to 'Edit', edit_order_path(order) %></td>
        <td><%= link_to 'Destroy', order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order', new_order_path %>
